{
  "name": "crystal",
  "rules": {
    "program": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "statements"
      }
    },
    "statements": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "statement"
        }
      ]
    },
    "statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "expression"
        }
      ]
    },
    "expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "nil"
        },
        {
          "type": "SYMBOL",
          "name": "bool"
        },
        {
          "type": "SYMBOL",
          "name": "integer"
        },
        {
          "type": "SYMBOL",
          "name": "float"
        },
        {
          "type": "SYMBOL",
          "name": "char"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "comment_directive"
            },
            {
              "type": "PATTERN",
              "value": ".*"
            }
          ]
        }
      ]
    },
    "comment_directive": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": ":ditto:"
        },
        {
          "type": "STRING",
          "value": ":nodoc:"
        },
        {
          "type": "STRING",
          "value": ":inherit:"
        }
      ]
    },
    "nil": {
      "type": "STRING",
      "value": "nil"
    },
    "bool": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "integer": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "[+-]?\\d[\\d_]*([iu](8|16|32|64))?"
        },
        {
          "type": "PATTERN",
          "value": "[+-]?0b[01_]+"
        },
        {
          "type": "PATTERN",
          "value": "[+-]?0o[0-7_]+"
        },
        {
          "type": "PATTERN",
          "value": "[+-]?0x[0-9a-fA-F_]+"
        }
      ]
    },
    "float": {
      "type": "PATTERN",
      "value": "[+-]?\\d[\\d_]*(\\.\\d[\\d_]*)?((e-?\\d+)|(f(32|64)))?"
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string_literal"
        },
        {
          "type": "SYMBOL",
          "name": "string_percent"
        }
      ]
    },
    "string_literal": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "string_interpolation"
              },
              {
                "type": "SYMBOL",
                "name": "string_escape"
              },
              {
                "type": "SYMBOL",
                "name": "string_content"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "ALIAS",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": "\\"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "string_literal"
                  }
                ]
              },
              "named": false,
              "value": ""
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "string_interpolation": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string_interpolation_start"
        },
        {
          "type": "SYMBOL",
          "name": "nil"
        },
        {
          "type": "SYMBOL",
          "name": "string_interpolation_end"
        }
      ]
    },
    "string_percent": {
      "type": "SEQ",
