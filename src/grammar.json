{
  "name": "crystal",
  "rules": {
    "program": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "statements"
      }
    },
    "statements": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "let x"
        },
        {
          "type": "STRING",
          "value": "let y"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "comment_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ":ditto:"
            },
            {
              "type": "STRING",
              "value": ":nodoc:"
            },
            {
              "type": "STRING",
              "value": ":inherit:"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "SYMBOL",
      "name": "comment_directive"
    },
    {
      "type": "PATTERN",
      "value": "(\\r|\\n|\\t| )"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

