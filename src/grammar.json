        {
          "type": "SYMBOL",
          "name": "string_percent_start"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "string_interpolation"
              },
              {
                "type": "SYMBOL",
                "name": "string_escape"
              },
              {
                "type": "SYMBOL",
                "name": "string_content"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "string_percent_end"
        }
      ]
    }
  },
  "extras": [
